<?xml version="1.0" encoding="UTF-8"?>
<Scenario xmlns="uri:/mil/tatrc/physiology/datamodel" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" contentVersion="BioGears_6.1.0-beta" xsi:schemaLocation="">
	<Name>ARM_GIVE_MEDS_NARCOTIC_FENTANYL_25MCG</Name>
	<Description>Give Patient Fentanyl for Pain</Description>
	<InitialParameters>
		<PatientFile>StandardMale.xml</PatientFile>
	</InitialParameters>	           	  

	<!-- ARM_GIVE_MEDS_NARCOTIC_FENTANYL_25MCG -->
	<!-- Give Patient Fentanyl for Pain -->
	<Action xsi:type="SubstanceBolusData" AdminRoute="Intravenous">
		<Substance>Fentanyl</Substance>
		<Concentration value="850" unit="ug/mL"/>
		<Dose value="20" unit="mL"/>
	</Action>    
</Scenario>


