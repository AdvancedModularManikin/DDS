<?xml version="1.0" encoding="UTF-8"?>
<Scenario xmlns="uri:/mil/tatrc/physiology/datamodel" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" contentVersion="BioGears_6.1.0-beta" xsi:schemaLocation="">
	<Name>ARM_GIVE_MEDS_SEDATIVE_PARALYTIC_MORPHINE_40MG</Name>
	<Description>Give patient Morphine 40 mg</Description>
	<InitialParameters>
		<PatientFile>StandardMale.xml</PatientFile>
	</InitialParameters>	           	  
	<!-- ARM_GIVE_MEDS_SEDATIVE_PARALYTIC_MORPHINE_40MG -->
	<!-- Give patient Morphine 40 mg -->
	<Action xsi:type="SubstanceBolusData" AdminRoute="Intravenous">
		<Substance>Morphine</Substance>
		<Concentration value="1000" unit="ug/mL"/>
		<Dose value="40" unit="mL"/>
	</Action>
</Scenario>
