<?xml version="1.0" encoding="UTF-8"?>
<Scenario xmlns="uri:/mil/tatrc/physiology/datamodel" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" contentVersion="BioGears_6.1.0-beta" xsi:schemaLocation="">
	<Name>ARM_GIVE_MEDS_SEDATIVE_PARALYTIC_SUCCINYLCHOLINE_140MG</Name>
	<Description>Give patient succinylcholine 140 mL</Description>
	<InitialParameters>
		<PatientFile>StandardMale.xml</PatientFile>
	</InitialParameters>	           	  

	<!-- ARM_GIVE_MEDS_SEDATIVE_PARALYTIC_SUCCINYLCHOLINE_140MG -->
	<!-- Give patient succinylcholine 140 mL-->          
	<Action xsi:type="SubstanceBolusData" AdminRoute="Intravenous">
		<Substance>Succinylcholine</Substance>
		<Concentration value="1000.0" unit="ug/mL"/>
		<Dose value="140.0" unit="mL"/>
	</Action>
</Scenario>

