	<Name>ARM_L_INFUSE_BLOOD_2_Units</Name>
	<Description>Infuse fluids</Description>
	<InitialParameters>
		<PatientFile>StandardMale.xml</PatientFile>
	</InitialParameters>	           	  

	<!-- ARM_INFUSE_BLOOD_2_Units -->
