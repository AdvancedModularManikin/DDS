<?xml version="1.0" encoding="UTF-8"?>
<Scenario xmlns="uri:/mil/tatrc/physiology/datamodel" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" contentVersion="BioGears_6.1.0-beta" xsi:schemaLocation="">
	<Name>ACUTE_STRESS</Name>
	<Description>Acute Stress</Description>
	<InitialParameters>
		<PatientFile>StandardMale.xml</PatientFile>
	</InitialParameters>	           	  
	<!-- ACUTE_STRESS -->
	<!-- Increase Epinephrine in Blood -->
    <Action xsi:type="SubstanceBolusData" AdminRoute="Intravenous">
        <Substance>Epinephrine</Substance>
        <Concentration value="1" unit="ug/mL"/>
        <Dose value="10" unit="mL"/>
	</Action>
</Scenario>
